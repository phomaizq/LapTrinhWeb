@model IEnumerable<WebApplication4.Models.Sach>
@{
    ViewBag.Title = "index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h2>index</h2>



<h2>Sach</h2>
<table border="1px">
    <tr>
        <th>Mã sách </th>
        <th>Mã loại </th>
        <th>Tên sách </th>
        <th>hình </th>
      
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            
            <td>@item.masach</td>
            <td>@item.maloai</td>
            <td>@item.tensach</td>
            <td>@item.hinh</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.masach }) |
                @Html.ActionLink("Detail", "Detail", new { id = item.masach }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.masach })
            </td>
        </tr>
    }
</table>
@Html.ActionLink("Them", "Create", "Sach")





